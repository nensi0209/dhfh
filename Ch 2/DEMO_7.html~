			<tr>
    				<td border=2>1</td>
    				<td colspan="3">2</td>
    			</tr>
  			<tr>
    				<td rowspan="3">3</td>
    				<td bgcolor=red>4</td>
				<td bgcolor=red>5</td>
    				<td rowspan="2"bgcolor=yellow>6</td>
  			</tr>
  			<tr>
    				<td bgcolor=red>7</td>
    				<td bgcolor=red>8</td>
  			</tr>
			<tr>
    				<td colspan="2" bgcolor=yellow>9</td>
    				<td bgcolor=yellow>10</td>
  			</tr>

			
